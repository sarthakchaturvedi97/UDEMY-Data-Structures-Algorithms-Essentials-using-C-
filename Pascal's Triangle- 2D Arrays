#include<bits/stdc++.h>
using namespace std;

vector<vector<int>> printPascal(int n)
{
    // your code goes here
    vector<vector<int>> result(n);
    for(int i=0;i<n;i++)
    {
        result[i].resize(i+1);
        result[i][0]=result[i][i]=1;
        
         for(int j=1;j<i;j++)
         {
                 result[i][j]=result[i-1][j-1]+result[i-1][j];
         }
    }
    return result;
}
 



