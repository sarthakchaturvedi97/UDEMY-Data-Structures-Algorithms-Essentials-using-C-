#include<bits/stdc++.h>
using namespace std;

vector<int> optimizedBubbleSort(vector<int> v){
    // your code  goes here
    for(int i=0;i<v.size()-1;i++)
    {
        bool check=false;
        for(int j=0;j<v.size()-i-1;j++)
        {
            if(v[j]>v[j+1])
            swap(v[j],v[j+1]);
            check=true;
        }
        if(!check)
        break;
        
    }
    return v;
}
